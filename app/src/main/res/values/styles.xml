<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- google_pixel_3xl___1 styles -->

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="splash_logo_style">
        <item name="android:layout_height">@dimen/splash_logo_dimen_height</item>
        <item name="android:layout_width">@dimen/splash_logo_dimen_width</item>
        <item name="android:src">@drawable/app_logo</item>
        <item name="android:layout_marginTop">@dimen/dp_40</item>
        <item name="contentDescription">@string/splash_image_cd</item>
    </style>

    <style name="nav_text_item">
        <item name="android:includeFontPadding">false</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="fontFamily">@font/poppins_medium</item>
    </style>

    <style name="nav_image_item">
        <item name="android:layout_marginLeft">12dp</item>
    </style>

    <style name="nav_ll_item">
        <item name="android:paddingTop">@dimen/dp_10</item>
        <item name="android:paddingBottom">@dimen/dp_10</item>
    </style>

    <style name="DialogTheme"></style>


</resources>